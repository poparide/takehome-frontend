{
  "expo": {
    "name": "Poparide",
    "slug": "Poparide",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "poparide",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "server",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "assets/fonts/ProximaSoft-Black.ttf",
            "assets/fonts/ProximaSoft-BlackIt.ttf",
            "assets/fonts/ProximaSoft-Bold.ttf",
            "assets/fonts/ProximaSoft-BoldIt.ttf",
            "assets/fonts/ProximaSoft-ExtraBold.ttf",
            "assets/fonts/ProximaSoft-ExtraBoldIt.ttf",
            "assets/fonts/ProximaSoft-Light.ttf",
            "assets/fonts/ProximaSoft-LightIt.ttf",
            "assets/fonts/ProximaSoft-Medium.ttf",
            "assets/fonts/ProximaSoft-MediumIt.ttf",
            "assets/fonts/ProximaSoft-Regular.ttf",
            "assets/fonts/ProximaSoft-RegularIt.ttf",
            "assets/fonts/ProximaSoft-SemiBold.ttf",
            "assets/fonts/ProximaSoft-SemiBoldIt.ttf",
            "assets/fonts/ProximaSoft-Thin.ttf",
            "assets/fonts/ProximaSoft-ThinIt.ttf"
          ]
        }
      ],
      ["expo-router", { "origin": "https://localhost:8081" }],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
